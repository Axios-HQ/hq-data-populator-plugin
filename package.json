{
  "name": "hq-data-populator-extension",
  "version": "1.0.0",
  "description": "Chrome extension for HQ Data Populator",
  "scripts": {
    "build": "webpack",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./dist/output.css",
    "watch:css": "tailwindcss -i ./src/styles/input.css -o ./dist/output.css --watch",
    "dev": "concurrently \"npm run watch:css\" \"webpack --watch\""
  },
  "dependencies": {
    "lucide-react": "^0.453.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@mui/material": "^5.11.4",  
    "@emotion/react": "^11.10.5", 
    "@emotion/styled": "^11.10.5" 
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^8.4.1",
    "concurrently": "^8.2.0",
    "css-loader": "^6.11.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.47",
    "postcss-loader": "^7.3.4",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.14",
    "webpack": "^5.95.0",
    "webpack-cli": "^4.10.0"
  }
}
